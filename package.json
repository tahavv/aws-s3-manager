{
  "name": "aws-s3-uploader",
  "version": "1.0.0",
  "private": true,
  "author": "Taha Ayari <taha.ayari@iteam-univ.tn> (https://tahavv.github.io/tahavv/)",
  "license": "MIT",
  "contributors": [],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:dev": "cross-env NEXT_PUBLIC_ENV=development next build",
    "build:prod": "cross-env NEXT_PUBLIC_ENV=production next build",
    "start": "next start",
    "start:dev": "cross-env NEXT_PUBLIC_ENV=development next start",
    "start:prod": "cross-env NEXT_PUBLIC_ENV=production next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/client-sns": "^3.787.0",
    "@aws-sdk/client-sqs": "^3.787.0",
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "formidable": "^3.5.3",
    "lucide-react": "^0.503.0",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-config-next": "^15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^8.0.0",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
